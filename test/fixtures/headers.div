		QRY
		QRY_SpeakerIsAvailable((GUIDSTRING)_Char)
		AND
		ObjectIsCharacter(_Char,1)
		AND
		QRY_SpeakerIsAvailable((GUIDSTRING)_Char, 0)
		THEN
		DB_NOOP(1);
		
		QRY
		QRY_SpeakerIsDead((GUIDSTRING)_Char)
		AND
		ObjectIsCharacter(_Char,1)
		AND
		CharacterIsDeadOrFeign((CHARACTERGUID)_Char,1)
		THEN

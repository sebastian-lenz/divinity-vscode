		THEN
		DB_NOOP(1);
		
		QRY
		QRY_SpeakerIsAvailable(NULL_00000000-0000-0000-0000-000000000000)
		THEN
		DB_NOOP(1);
		
		QRY
		QRY_CharacterIsNotDisabled((CHARACTERGUID)_Character)

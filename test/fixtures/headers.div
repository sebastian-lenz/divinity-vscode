		PROC
		ProcGetClosestAvailableCharacterTo((CHARACTERGUID)_Obj,0,0,(CHARACTERGUID)_Player,(CHARACTERGUID)_ExceptPlayer)
		AND 
		DB_IsPlayer(_Char)
		AND
		_Char != _ExceptPlayer

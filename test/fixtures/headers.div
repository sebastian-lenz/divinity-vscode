		PROC
		ProcGetClosestAvailableCharacterTo((CHARACTERGUID)_Obj,1,1,(CHARACTERGUID)_Player,(CHARACTERGUID)_ExceptPlayer)
		AND 
		DB_IsPlayer(_Char)
		AND
		_Char != _ExceptPlayer
		AND
		CharacterCanSee(_Obj,(CHARACTERGUID)_Char,1)

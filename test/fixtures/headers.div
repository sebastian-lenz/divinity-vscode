		OpenWaypointUIForFlee((CHARACTERGUID)_Player,(STRING)_WaypointCurrent,(ITEMGUID)_Item)
		THEN
		OpenWaypointUI((CHARACTERGUID)_Player,_WaypointCurrent,_Item,1);
		//END_REGION Waypoint
		
	}
	EXIT
	{
		
	}
}

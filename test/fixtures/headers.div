	}
	KB
	{
		IF
		CharacterTeleportToWaypoint(_Char,_Trigger)
		AND
		GetPosition(_Trigger,_X,_Y,_Z)
		THEN
		TeleportToPosition(_Char,_X,_Y,_Z,"",1,1);
		
		IF
		CharacterTeleportToFleeWaypoint(_Char,_Trigger)
		AND
		GetPosition(_Trigger,_X,_Y,_Z)
		THEN

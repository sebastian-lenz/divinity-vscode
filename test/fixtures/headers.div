		AND
		IsTagged(_Witness,"ANIMAL",0) //Animals should not investigate should be disabled in excel
		THEN
		ProcMakeNPCHostile(_Witness,_Criminal);
		//END_REGION
		//END_REGION
		
		//REGION Trespassing
		
